{
  "name": "webhook-tester",
  "version": "1.0.0",
  "description": "Application for testing and debugging webhooks with dynamic endpoints",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "build": "npm run client:build"
  },
  "keywords": [
    "webhook",
    "testing",
    "api",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "sqlite": "^5.0.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
